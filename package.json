    "notify-osd",
    "growl"
  ],
  "homepage": "https://github.com/mikaelbr/gulp-notify",
  "bugs": "https://github.com/mikaelbr/gulp-notify/issues",
  "author": {
    "name": "Mikael Brevik",
    "email": "mikaelbre@gmail.com",
    "url": "https://github.com/mikaelbr"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mikaelbr/gulp-notify.git"
  },
  "scripts": {
